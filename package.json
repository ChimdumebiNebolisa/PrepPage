{
  "name": "prep-page",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:ci": "vitest run",
    "verify:grid": "tsx scripts/verify-grid-routes.ts",
    "verify:grid:ps": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"npm run verify:grid\"",
    "grid:health": "node -e \"fetch('http://localhost:3000/api/grid/health').then(r=>r.json()).then(d=>console.log(JSON.stringify(d,null,2))).catch(e=>console.error('Error:',e.message))\"",
    "grid:teams": "node -e \"fetch('http://localhost:3000/api/teams?q=faze').then(r=>r.json()).then(d=>console.log(JSON.stringify(d,null,2))).catch(e=>console.error('Error:',e.message))\"",
    "grid:scout": "node -e \"fetch('http://localhost:3000/api/teams?q=faze&limit=1').then(r=>r.json()).then(d=>{const team=d.teams[0];if(!team)throw new Error('No team found');return fetch('http://localhost:3000/api/scout',{method:'POST',headers:{'Content-Type':'application/json'},body:JSON.stringify({teamId:team.id,game:'lol'})}).then(r=>r.json()).then(d=>console.log(JSON.stringify(d,null,2)));}).catch(e=>console.error('Error:',e.message))\"",
    "prepare": "husky"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^2.0.0"
  }
}
